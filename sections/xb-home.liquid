  <!DOCTYPE html>
<html>
<head>
<style>
/* Custom Image Pagination */
.kpr-custom-pagination {
    position: fixed;
    top: 50%;
    left: calc(50% - 320px);
    transform: translateY(-50%);
    z-index: 9999;
    display: flex;
    flex-direction: column;
    gap: 15px;
    padding: 20px 10px;
    background: rgba(0, 0, 0, 0.3);
    backdrop-filter: blur(10px);
    border-radius: 15px;
    border: 1px solid rgba(255, 255, 255, 0.1);
}

.kpr-pagination-item {
    width: 80px;
    height: 80px;
    border-radius: 10px;
    overflow: hidden;
    cursor: pointer;
    transition: all 0.3s ease;
    border: 2px solid transparent;
    position: relative;
}

.kpr-pagination-item:hover {
    transform: scale(1.05);
    border-color: #12b939;
    box-shadow: 0 5px 15px rgba(18, 185, 57, 0.3);
}

.kpr-pagination-item.active {
    border-color: #12b939;
    box-shadow: 0 0 20px rgba(18, 185, 57, 0.5);
}

.kpr-pagination-item.active::after {
    content: '✓';
    position: absolute;
    top: 5px;
    right: 5px;
    background: #12b939;
    color: white;
    width: 20px;
    height: 20px;
    border-radius: 50%;
    display: flex;
    align-items: center;
    justify-content: center;
    font-size: 12px;
    font-weight: bold;
}

.kpr-pagination-img {
    width: 100%;
    height: 100%;
    object-fit: cover;
    transition: transform 0.3s ease;
}

.kpr-pagination-item:hover .kpr-pagination-img {
    transform: scale(1.1);
}

/* Hide original pagination */
#kpr-pagination-1 {
    display: none !important;
}

/* Responsive */
@media (max-width: 768px) {
    .kpr-custom-pagination {
        left: 20px;
        gap: 10px;
        padding: 15px 8px;
    }
    
    .kpr-pagination-item {
        width: 60px;
        height: 60px;
    }
}
</style>
</head>
<body>

<!-- Custom Pagination HTML -->
<div class="kpr-custom-pagination" id="kpr-custom-pagination">
    <!-- Items will be generated by JavaScript -->
</div>

<script>
// Custom Pagination Controller
class KPRCustomPagination {
    constructor() {
        this.carousel = document.querySelector('.xo-carousel');
        this.originalPagination = document.getElementById('kpr-pagination-1');
        this.customContainer = document.getElementById('kpr-custom-pagination');
        this.currentIndex = 0;
        
        // Configuration - Update these images for your products
        this.paginationData = [
            {
                image: 'https://cdn.shopify.com/s/files/1/0580/6994/2352/files/cue-vertical_2.png?v=1752891681&width=200',
                alt: 'Pool Cue 1'
            },
            {
                image: 'https://cdn.shopify.com/s/files/1/0580/6994/2352/files/cue-vertical_2.png?v=1752891681&width=200',
                alt: 'Pool Cue 2'
            }
            // Add more items as needed
        ];
        
        this.init();
    }
    
    init() {
        if (!this.carousel || !this.customContainer) return;
        
        this.createCustomPagination();
        this.bindEvents();
        this.syncWithOriginal();
    }
    
    createCustomPagination() {
        this.customContainer.innerHTML = '';
        
        this.paginationData.forEach((item, index) => {
            const paginationItem = document.createElement('div');
            paginationItem.className = `kpr-pagination-item ${index === 0 ? 'active' : ''}`;
            paginationItem.dataset.index = index;
            
            paginationItem.innerHTML = `
                <img src="${item.image}" alt="${item.alt}" class="kpr-pagination-img" loading="lazy">
            `;
            
            this.customContainer.appendChild(paginationItem);
        });
    }
    
    bindEvents() {
        // Custom pagination click
        this.customContainer.addEventListener('click', (e) => {
            const item = e.target.closest('.kpr-pagination-item');
            if (!item) return;
            
            const index = parseInt(item.dataset.index);
            this.goToSlide(index);
        });
        
        // Watch for original carousel changes
        if (this.carousel) {
            const observer = new MutationObserver(() => {
                this.syncFromOriginal();
            });
            
            observer.observe(this.carousel, {
                subtree: true,
                attributes: true,
                attributeFilter: ['xo-active']
            });
        }
    }
    
    goToSlide(index) {
        // Trigger original pagination
        const originalBullets = document.querySelectorAll('xo-carousel-bullet');
        if (originalBullets[index]) {
            originalBullets[index].click();
        }
        
        this.updateActiveState(index);
    }
    
    updateActiveState(index) {
        // Update custom pagination
        this.customContainer.querySelectorAll('.kpr-pagination-item').forEach((item, i) => {
            item.classList.toggle('active', i === index);
        });
        
        this.currentIndex = index;
    }
    
    syncFromOriginal() {
        const activeBullet = document.querySelector('xo-carousel-bullet[xo-active]');
        if (activeBullet) {
            const index = parseInt(activeBullet.getAttribute('xo-index') || '0');
            this.updateActiveState(index);
        }
    }
    
    syncWithOriginal() {
        // Initial sync
        setTimeout(() => {
            this.syncFromOriginal();
        }, 100);
    }
}

// Initialize when DOM is ready
document.addEventListener('DOMContentLoaded', () => {
    // Wait for carousel to initialize
    setTimeout(() => {
        new KPRCustomPagination();
    }, 500);
});

// Also initialize on window load as backup
window.addEventListener('load', () => {
    setTimeout(() => {
        if (!document.getElementById('kpr-custom-pagination').children.length) {
            new KPRCustomPagination();
        }
    }, 1000);
});
</script>

</body>
</html>
