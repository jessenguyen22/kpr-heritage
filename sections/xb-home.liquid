  <!-- Lottie Animation with Hover Effect -->
<a href="#traditional-section" class="lottie-interactive-container" style="position: relative; width: 100%; height: 550px; cursor: pointer; display: block; text-decoration: none;">
  <!-- Lottie Container -->
  <div id="tiger-lottie" style="width: 100%; height: 100%;"></div>
</a>

<script>
(function() {
  // Wait for Lottie library to load
  if (typeof lottie === 'undefined') {
    setTimeout(arguments.callee, 100);
    return;
  }
  
  const tigerAnimation = lottie.loadAnimation({
    container: document.getElementById('tiger-lottie'),
    renderer: 'svg',
    loop: false,
    autoplay: false,
    path: 'https://cdn.shopify.com/s/files/1/0580/6994/2352/files/Tiger.json?v=1752657585'
  });
  
  const container = document.querySelector('.lottie-interactive-container');
  let isHovering = false;
  
  // Wait for animation to load then STOP completely
  tigerAnimation.addEventListener('DOMLoaded', function() {
    tigerAnimation.goToAndStop(0, true);
    console.log('Tiger animation loaded - staying idle');
  });
  
  // Hover events
  container.addEventListener('mouseenter', function() {
    isHovering = true;
    tigerAnimation.goToAndStop(0, true);
    tigerAnimation.play();
    console.log('Hover started - playing full animation');
  });
  
  container.addEventListener('mouseleave', function() {
    isHovering = false;
    tigerAnimation.stop();
    tigerAnimation.goToAndStop(0, true);
    console.log('Hover ended - back to static');
  });
  
  // Remove the manual click handler since <a> tag handles navigation
  
  // Cleanup
  window.addEventListener('beforeunload', function() {
    tigerAnimation.destroy();
  });
})();
</script>

<style>
.lottie-interactive-container {
  transition: transform 0.5s ease;
  animation: subtleBreathing 4s ease-in-out infinite;
}

.lottie-interactive-container:hover {
  transform: scale(1.05);
  animation: none;
}

@keyframes subtleBreathing {
  0%, 100% { transform: scale(1); }
  50% { transform: scale(1.03); }
}
</style>
